# 预发布环境配置
NODE_ENV=staging
API_BASE_URL=https://staging-api.findvalue.com
API_TIMEOUT=20000
DEBUG_MODE=false
LOG_LEVEL=warn

# 应用配置
APP_NAME=FindValue Staging
APP_VERSION=1.0.0-staging
APP_ID_SUFFIX=.staging
VERSION_SUFFIX=-staging
BUNDLE_IDENTIFIER=com.findvalue.staging

# 数据库配置
DB_HOST=staging-db.findvalue.com
DB_PORT=5432
DB_NAME=findvalue_staging
DB_USER=staging_user
DB_PASSWORD=staging_password

# 第三方服务配置
GOOGLE_MAPS_API_KEY=your_staging_google_maps_key
FIREBASE_PROJECT_ID=findvalue-staging
FIREBASE_API_KEY=your_staging_firebase_key

# 功能开关
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true
ENABLE_DEBUG_TOOLS=false
