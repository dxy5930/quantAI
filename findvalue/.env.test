# 测试环境配置
NODE_ENV=test
API_BASE_URL=https://test-api.findvalue.com
API_TIMEOUT=15000
DEBUG_MODE=false
LOG_LEVEL=info

# 应用配置
APP_NAME=FindValue QA
APP_VERSION=1.0.0-qa
APP_ID_SUFFIX=.qa
VERSION_SUFFIX=-qa
BUNDLE_IDENTIFIER=com.findvalue.qa

# 数据库配置
DB_HOST=test-db.findvalue.com
DB_PORT=5432
DB_NAME=findvalue_test
DB_USER=test_user
DB_PASSWORD=test_password

# 第三方服务配置
GOOGLE_MAPS_API_KEY=your_test_google_maps_key
FIREBASE_PROJECT_ID=findvalue-test
FIREBASE_API_KEY=your_test_firebase_key

# 功能开关
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true
ENABLE_DEBUG_TOOLS=false
