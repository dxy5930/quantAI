# 开发环境配置
NODE_ENV=development
API_BASE_URL=http://localhost:3000
API_TIMEOUT=10000
DEBUG_MODE=true
LOG_LEVEL=debug

# 应用配置
APP_NAME=FindValue Dev
APP_VERSION=1.0.0-dev
APP_ID_SUFFIX=.dev
VERSION_SUFFIX=-dev
BUNDLE_IDENTIFIER=com.findvalue.dev

# 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_NAME=findvalue_dev
DB_USER=postgres
DB_PASSWORD=password

# 第三方服务配置
GOOGLE_MAPS_API_KEY=your_dev_google_maps_key
FIREBASE_PROJECT_ID=findvalue-dev
FIREBASE_API_KEY=your_dev_firebase_key

# 功能开关
ENABLE_ANALYTICS=false
ENABLE_CRASH_REPORTING=false
ENABLE_DEBUG_TOOLS=true
