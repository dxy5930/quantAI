# 环境配置模板文件
# 复制此文件为 .env.dev, .env.test, .env.staging, .env.prod 并根据需要修改

# 环境类型
NODE_ENV=development

# API配置
API_BASE_URL=http://localhost:3000
API_TIMEOUT=10000

# 调试配置
DEBUG_MODE=true
LOG_LEVEL=debug

# 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_NAME=findvalue_dev
DB_USER=postgres
DB_PASSWORD=password

# 第三方服务配置
GOOGLE_MAPS_API_KEY=your_google_maps_key
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_API_KEY=your_firebase_api_key

# 应用配置
APP_NAME=FindValue
APP_VERSION=1.0.0
BUNDLE_IDENTIFIER=com.findvalue

# 功能开关
ENABLE_ANALYTICS=false
ENABLE_CRASH_REPORTING=false
ENABLE_DEBUG_TOOLS=true
