# 开发环境配置
NODE_ENV=development
PORT=3001
API_PREFIX=api/v1

# 数据库配置
DB_HOST=localhost
DB_PORT=3366
DB_USERNAME=root
DB_PASSWORD=restosuite
DB_DATABASE=chaogu

# JWT配置
JWT_SECRET=dev_jwt_secret_key_change_in_production_12345678
JWT_EXPIRES_IN=24h

# CORS配置
