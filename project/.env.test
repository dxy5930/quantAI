# 测试环境配置
VITE_APP_ENV=test
VITE_APP_TITLE=智能策略回测平台 (测试版)
VITE_APP_VERSION=1.0.0-test
VITE_APP_DESCRIPTION=智能策略回测平台 - 测试环境

# API配置
VITE_API_BASE_URL=http://localhost:3002
VITE_PYTHON_API_TIMEOUT=120000
VITE_API_PREFIX=/api/v1
VITE_API_RETRY_COUNT=2
VITE_PYTHON_API_BASE_URL=http://localhost:8000

# 开发配置
VITE_DEV_PORT=5174
VITE_DEV_HOST=0.0.0.0
VITE_DEV_OPEN=false
VITE_DEV_OVERLAY=false

# 构建配置
VITE_BUILD_SOURCEMAP=false
VITE_BUILD_DROP_CONSOLE=true
VITE_BUILD_MINIFY=true

# 功能开关
VITE_ENABLE_MOCK=false
VITE_ENABLE_DEVTOOLS=false
VITE_ENABLE_PWA=true
VITE_ENABLE_HMR=false

# 第三方服务配置
VITE_ENABLE_ANALYTICS=true
VITE_ANALYTICS_ID=TEST-ANALYTICS-ID
VITE_ENABLE_SENTRY=true
VITE_SENTRY_DSN=https://test-sentry-dsn@sentry.io/test-project

# 主题配置
VITE_DEFAULT_THEME=light
VITE_THEME_PRIMARY=#1890ff
VITE_THEME_SECONDARY=#722ed1

# 调试配置
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=warn
VITE_SHOW_PERFORMANCE=false

# 测试配置
VITE_TEST_MODE=true
VITE_TEST_API_DELAY=1000
VITE_TEST_USER_EMAIL=test@chaogu.com
